{
  "name": "soap-cds",
  "version": "1.0.0",
  "description": "Consuming SOAP",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3.12.0",
    "@sap-cloud-sdk/http-client": "^3.12.0",
    "@sap/cds": "^7",
    "express": "^4",
    "soap": "^1.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap-cloud-sdk/test-util": "^3.12.0"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "sayHello": {
        "kind": "SOAP",
        "credentials": {
          "name": "soap-server",
          "url": "http://127.0.0.1:8001",
          "path": "/SayHello"
        }
      },
      "destinations": true
    }
  }
}
